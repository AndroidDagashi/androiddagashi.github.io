{
  "name": "androiddagashi",
  "version": "1.0.0",
  "description": "site generator for AndroidDagashi",
  "repository": "git@github.com:yshrsmz/androiddagashi.git",
  "author": "Yasuhiro Shimizu <the.phantom.bane@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt-ts",
    "build": "cross-env NODE_ENV=production nuxt-ts build",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "generateJson": "node buildscripts/githubapi-fetcher/index.js",
    "generateRssFeed": "node buildscripts/feed-generator/index.js",
    "generate": "cross-env NODE_ENV=production nuxt-ts generate",
    "deploy": "push-dir --dir=dist --branch=master --cleanup --verbose",
    "deployUpstream": "push-dir --dir=dist --remote=upstream --branch=master --cleanup --verbose",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.5.4",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/vuetify": "^1.9.1",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^1.21.0",
    "@types/markdown-it": "^0.0.9",
    "@types/node": "^12.12.5",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "apollo-fetch": "^0.7.0",
    "cross-env": "^6.0.3",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-vue": "^6.1.1",
    "feed": "^4.0.0",
    "graphql": "^14.5.8",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "push-dir": "^0.4.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.3.5",
    "@nuxtjs/axios": "^5.9.0",
    "lodash.flatmap": "^4.5.0",
    "luxon": "^1.21.3",
    "markdown-it": "^10.0.0",
    "modern-normalize": "^0.5.0",
    "nuxt": "^2.11.0",
    "nuxt-property-decorator": "^2.5.0",
    "vue": "^2.6.11",
    "vue-server-renderer": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.1.15"
  }
}
