    "nuxt": "^1.4.0",
    "nuxt-class-component": "^1.2.1",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-markdown": "^2.2.4",
    "vue-property-decorator": "^6.0.0",
    "vuetify": "^1.0.16",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0"
