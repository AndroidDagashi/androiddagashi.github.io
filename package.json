  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "build": "cross-env NODE_ENV=production nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start",
    "generateJson": "node buildscripts/githubapi-fetcher/index.js",
    "generateRssFeed": "node buildscripts/feed-generator/index.js",
    "generate": "cross-env NODE_ENV=production nuxt generate",
    "deploy": "push-dir --dir=dist --branch=master --cleanup --verbose",
    "deployUpstream": "push-dir --dir=dist --remote=upstream --branch=master --cleanup --verbose",
    "lint": "eslint '**/*.{ts,vue,js}'",
    "lintAndFix": "eslint --fix '**/*.{ts,vue,js}'"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.107",
