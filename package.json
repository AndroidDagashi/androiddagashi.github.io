    "patch-package": "8.0.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.6",
    "volar-service-prettier": "0.0.61"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "vue": "2.7.16",
