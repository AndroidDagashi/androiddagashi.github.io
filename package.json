    "ajv": "^6.4.0",
    "apollo-fetch": "^0.7.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-typescript": "^1.0.2",
    "eslint-plugin-typescript": "^0.11.0",
    "eslint-plugin-vue": "4.5.0",
    "feed": "^1.1.1",
    "graphql": "^0.13.2",
    "js-yaml": "^3.11.0",
    "push-dir": "^0.4.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
